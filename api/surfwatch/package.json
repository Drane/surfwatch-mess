{
  "name": "surfwatch",
  "version": "1.0.0",
  "description": "api for surfwatch",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "dev": "concurrently --kill-others -n build,mocha \"npm:build:watch\" \"npm:test:watch\"",
    "build": "npm run build:dist8 && npm run build:dist10",
    "build:apidocs": "lb-apidocs",
    "build:current": "lb-tsc",
    "build:dist8": "lb-tsc es2017",
    "build:dist10": "lb-tsc es2018",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist*",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run prettier:fix && npm run tslint:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build:current",
    "test": "lb-mocha --allow-console-logs \"DIST/test\"",
    "test:watch": "lb-mocha dist8/test/**/*.js --watch dist8/test/**/*.js && npm run posttest",
    "test:watch-logs": "lb-mocha --allow-console-logs dist8/test/**/*.js --watch dist8/test/**/*.js && npm run posttest",
    "posttest": "npm run lint",
    "prestart": "npm run build:current",
    "start": "node .",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist*/src",
    "dist*/index*",
    "src"
  ],
  "dependencies": {
    "@loopback/boot": "^0.14.5",
    "@loopback/context": "^0.13.1",
    "@loopback/core": "^0.11.16",
    "@loopback/dist-util": "^0.3.7",
    "@loopback/openapi-v3": "^0.15.4",
    "@loopback/rest": "^0.25.4"
  },
  "devDependencies": {
    "@loopback/build": "^0.7.6",
    "@loopback/testlab": "^0.15.1",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.11.4",
    "mocha": "^5.2.0",
    "source-map-support": "^0.5.9"
  }
}
